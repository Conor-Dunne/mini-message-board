<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <header>
      <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    </header>
    <div >
      
      <ul class="feed">
        <button>
          <a href="/new" rel="noopener noreferrer">Add Message</a>
        </button>
        <% messages.forEach(function(message) { %>
          <li><div class="message-box">
            <h2><%= message.user %> </h2>
            <p><%=message.added %></p>
             <p class="comment"><%= message.text %></p>
          </div></li>
        <% }); %>
      </ul>
    </div>
    
  </body>
</html>
